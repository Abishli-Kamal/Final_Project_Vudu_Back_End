@model Slider
@{
    ViewData["Title"] = "Detail";
}

<table class="table table-dark">
    <thead>
        <tr>

            <th scope="col">Id</th>
            <th scope="col">Image</th>
        </tr>
    </thead>
    <tbody>
        <tr>

            <td>@Model.Id</td>
            <td>
               <img class="w-100" src="~/assets/img/Slider/@Model.Image"/>
            </td>
        </tr>
</table>

