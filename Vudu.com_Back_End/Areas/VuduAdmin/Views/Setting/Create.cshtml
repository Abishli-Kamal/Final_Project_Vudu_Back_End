@model Setting
<form class="mt-5" method="post" enctype="multipart/form-data">
    <div>
        <label>Key:</label>
        <input asp-for="Key" class="form-control" /><br />
        <span class="text-danger" asp-validation-for="Key"></span>
    </div>
    <div>
        <label>Value:</label>
        <input asp-for="Value" accept="image/*" class="form-control" /><br />
        <span class="text-danger" asp-validation-for="Value"></span>
    </div>
    <div>
        <label>Photo:</label>
        <input asp-for="Photo" accept="image/*" class="form-control" /><br />
        <span class="text-danger" asp-validation-for="Photo"></span>
    </div>

        <div>
            <button class="btn btn-dark" type="submit">Create</button>
        </div>
</form>
