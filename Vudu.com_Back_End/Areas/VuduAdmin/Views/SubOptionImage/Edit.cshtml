@model SubOptionImage
@{
    ViewData["Title"] = "Create";
}


<div asp-validation-summary="ModelOnly" class="alert alert-danger"></div>
<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label for="name">Title</label>
        <input asp-for="Title" class="form-control" id="name">
        <span class="text-danger" asp-validation-for="Title"></span>
    </div>

    <div class="form-group">
        <label for="name">MainOptionId</label>
        <input asp-for="MainOptionId" class="form-control" id="name">
        <span class="text-danger" asp-validation-for="MainOptionId"></span>
    </div>

        <div class="form-group">
        <label for="name">Image</label>
        <input asp-for="Photo" class="form-control" id="name">
        <span class="text-danger" asp-validation-for="Photo"></span>
    </div>
 
    <div>
       <button type="submit" class="btn btn-primary mt-3">Edit</button>
    </div>
</form>
  
