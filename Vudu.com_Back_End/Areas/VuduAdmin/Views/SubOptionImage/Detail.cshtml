@model SubOptionImage
@{
    ViewData["Title"] = "Detail";
}

<table class="table table-dark">
    <thead>
        <tr>

            <th scope="col">Id</th>
            <th scope="col">Title</th>
            <th scope="col">Image</th>
        </tr>
    </thead>
    <tbody>
        <tr>

            <td>@Model.Id</td>
            <td>@Model.Title</td>
            <td>
                <img class="w-50" src="~/assets/img/Mov-Img-Navbar/@Model.Image">
            </td>
        </tr>
</table>

